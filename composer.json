{
    "name": "marcoslopez95/wrap-and-action-package",
    "description": "a package for create action classes and wrap classes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Manu\\WrapAndActionPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manu\\WrapAndActionPackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "role": "Developer",
            "name": "Marcos M. LÃ³pez",
            "email": "marcoslopez1895@gmail.com",
            "homepage": "https://marcoslopez95.github.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.36"
    }
}
